---
import type { HTMLAttributes } from "astro/types";
import { cn } from "~/functions";
type Props = HTMLAttributes<"h1">;
const { class: className, ...rest } = Astro.props;
---

<h1 class={cn("font-extrabold leading-9 tracking-tight text-gray-900 dark:text-gray-100 text-5xl sm:leading-10 md:text-6xl md:leading-14", className)} {...rest}>
  <slot />
</h1>
